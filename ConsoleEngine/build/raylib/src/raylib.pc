prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib64
includedir=${prefix}/include

Name: raylib
Description: Simple and easy-to-use library to enjoy videogames programming
URL: http://github.com/raysan5/raylib
Version: 3.0.0
Libs: -L${libdir} -lraylib -L/usr/lib64 -lGL  -lrt -lm -ldl -lrt -lm -ldl -lrt -lm -ldl -lrt -lm -ldl
Libs.private: -L/usr/lib64 -lGL  -lrt -lm -ldl -lrt -lm -ldl -lrt -lm -ldl -lrt -lm -ldl
Requires.private:  x11 x11 x11 x11
Cflags: -I${includedir}
